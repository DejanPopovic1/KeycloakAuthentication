@{
    ViewData["Title"] = "Protected resource";
}
<h1>@ViewData["Title"]</h1>

<p>This is a protected resource.</p>


<table class="table table-striped table-bordered">

    @foreach (var claim in User.Claims)
    {
        <tr>
            <td>@claim.Type</td>
            <td>@claim.Value</td>
        </tr>
    }
</table>